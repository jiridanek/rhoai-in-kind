{
  "ignore": [
    "**/*.css"
  ],
  "include": [
    "./node_modules/specific-package/**/*"
  ],
  "presets": [
    ["@babel/preset-env", {
      "targets": { "node": "current" },
      "modules": "auto"
    }],
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "corejs": false,
      "helpers": true,
      "regenerator": true
    }]
  ]
}
